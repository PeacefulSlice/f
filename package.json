{
  
  "main": "truffle-config.js",
  
  "directories": {
  
    
  "test": "test"
  
  },
  
  "scripts": {
  
    
  "start": "truffle console --network",
  
    
  "init": "truffle init",
  
    
  "build": "truffle compile",
  
    
  "test": "truffle test --network test",
  
    
  "testrinkeby": "truffle test --network rinkeby",
  
    
  "test-hamster": "concurrently --kill-others \"npm run ganache\" \"npm run test test/HamsterTest.js\"",
  
  "compile": "truffle compile",
  
  "ganache": "ganache-cli --allowUnlimitedContractSize -q --networkId 5777  -l 40000000 -a ${ACCOUNT_AMOUNT:-10}",
    
  "migrate": "truffle migrate",
    
  "migraterinkeby": "truffle migrate --network rinkeby --reset --skip-dry-run",
    
  "migratetest": "truffle migrate --network test --reset"
  
  },
  
  "license": "MIT",
  
  "dependencies": {
  
    
  "@openzeppelin/contracts": "^4.4.0",
  
    
  "@openzeppelin/contracts-upgradeable": "^4.4.0",
  
    
  "@openzeppelin/test-helpers": "^0.5.12",
  
    
  "@remix-project/remixd": "^0.3.5",
  
    
  "base64-sol": "^1.1.0",
  
    
  "bn-chai": "^1.0.1",
  
    
  "chai": "^4.2.0",
  
    
  "chai-match": "^1.1.1",
  
    
  "ganache-cli": "^6.11.0",
  
    
  "js-big-decimal": "^1.3.1",
  
    
  "truffle": "^5.1.43",
  
    
  "truffle-privatekey-provider": "^1.5.0",
  
    
  "tslib": "^2.0.1",
  
    
  "web3": "^1.3.4"
  
  },
  
  "devDependencies": {
  
    
  "@truffle/hdwallet-provider": "^1.7.0",
  
    
  "concurrently": "^5.3.0"
  
  }
}
